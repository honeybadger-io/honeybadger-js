  "license": "MIT",
  "homepage": "https://github.com/honeybadger-io/honeybadger-js",
  "author": {
    "name": "Joshua Wood",
    "email": "josh@honeybadger.io"
  },
  "main": "dist/honeybadger.cjs.js",
  "module": "dist/honeybadger.esm.js",
  "browser": "dist/honeybadger.js",
