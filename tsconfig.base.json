{
  "compilerOptions": {
    "allowJs": false,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "declaration": true,
    "declarationMap": true,
    "stripInternal": true,
    "skipLibCheck": true
  }
}
